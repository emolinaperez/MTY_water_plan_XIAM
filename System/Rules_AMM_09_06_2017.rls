    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Campo Mina.Pumped Flow" [] := IF ( $ "Campo Mina.Storage" [@"t - 1"] > 10.00000000 "MCM" )
 THEN
