      $ "Pozos Area Metropolitana.Pumped Flow" [@"24:00:00 September Max DayOfMonth, Current Year"] := IF ( $ "Pozos Area Metropolitana.Storage" [@"t - 1"] > 1.00000000 "MCM" )
