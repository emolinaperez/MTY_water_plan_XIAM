      $ "El Cuchillo.Regulated Spill" [@"24:00:00 November Max DayOfMonth, Current Year"] := IF ( $ "El Cuchillo.Storage" [@"t - 1"] > $ "DataObj1.NIVEL_CUCHILLO_TRASVASE" [] AND $ "Marte R Gomez.Storage" [@"t - 1"] < 700.00000000 "MCM" )
