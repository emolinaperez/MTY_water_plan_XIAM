    ACTIVE               FALSE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Las Blancas.Diversion" [] := IF ( $ "Las Blancas.Storage" [@"t - 1"] > 15.52000000 "MCM" )
 THEN
  $ "DataObj1.Trasvase Las Blancas" []
 ELSE
  0.00000000 "cms"
 ENDIF;

      $ "Las Blancas.Release" [] := 0.00000000 "cms";

    END;

    RULE                 "Caudal Eco_LasBlancas";
    DESCRIPTION          "";
