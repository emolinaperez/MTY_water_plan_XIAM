    RULE                 "Rule_MRGomez";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Marte R Gomez.Release" [] := IF ( $ "Marte R Gomez.Storage" [@"t - 1"] > 40.00000000 "MCM" )
 THEN
  35.00000000 "MCM/month"
 ELSE
  0.00000000 "MCM/month"
 ENDIF;

    END;

    RULE                 "Extracciones_BAires";
