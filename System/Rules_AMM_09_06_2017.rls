    BEGIN

      $ "Sistema Santiago Huajuco.Pumped Flow" [] := IF ( $ "Sistema Santiago Huajuco.Storage" [@"t - 1"] > 4.00000000 "MCM" )
 THEN
  1.10000000 "cms"
 ELSE
  0.00000000 "cms"
 ENDIF;

