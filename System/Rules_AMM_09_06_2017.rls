  POLICY_GROUP   "AMM";
  DESCRIPTION    "";
  ACTIVE         TRUE;
  BEGIN

    RULE                 "Demanda Monterrey";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Area Metropolitana de Monterrey.Diversion Requested" [] := $ "Analisis.Demanda Monterrey_2" [];

    END;

    RULE                 "Agua derivada a Pesqueria";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Plantas de Tratamiento.Normal Flow" [] := $ "Plantas de Tratamiento.Gage Inflow" [] - $ "Plantas de Tratamiento.Condition One" [] + $ "DataObj2.Pozos Privados" [];

    END;

    RULE                 "Agua tratada a la industria";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Plantas de Tratamiento.Condition One" [] := IF ( $ "Plantas de Tratamiento.Gage Inflow" [] > 6.00000000 "cms" )
 THEN
  $ "Plantas de Tratamiento.Gage Inflow" [] - 6.00000000 "cms"
 ELSE
  0.00000000 "cms"
 ENDIF;

    END;

    RULE                 "Oferta total actual";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Area Metropolitana de Monterrey.Incoming Available Water" [] := $ "DiversionObject1.Diversion" [] + $ "DiversionObject2.Diversion" [] + $ "DiversionObject3.Diversion" [] + $ "Campo Mina.Pumped Flow" [] + $ "Campo Buenos Aires.Pumped Flow" [] + ( $ "Pozos Area Metropolitana.Pumped Flow" [] + $ "Area Metropolitana de Monterrey.Available Supplemental Water" [] ) + $ "Sistema Santiago Huajuco.Pumped Flow" [];

    END;

  END;

  POLICY_GROUP   "Alternativas";
  DESCRIPTION    "";
  ACTIVE         TRUE;
  NAMES_COLOR  "#ff5500";
  BEGIN

    RULE                 "Rule_VGuerrero";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "DiversionObject4.Diversion" [] := IF ( $ "Vicente Guerrero.Pool Elevation" [@"t - 1"] > 131.00000000 "m" )
 THEN
  $ "DataObj3.Extracciones_VGuerrero" []
 ELSE
  0.00000000 "cms"
 ENDIF;

      $ "Vicente Guerrero.Release" [] := IF ( $ "Area Metropolitana de Monterrey.Diversion Requested" [] > $ "Area Metropolitana de Monterrey.Diversion" [] )
 THEN
  IF ( $ "Area Metropolitana de Monterrey.Diversion Requested" [] - $ "Area Metropolitana de Monterrey.Diversion" [] < 5.00000000 "cms" )
  THEN
   $ "Area Metropolitana de Monterrey.Diversion Requested" [] - $ "Area Metropolitana de Monterrey.Diversion" []
  ELSE
   IF ( $ "Vicente Guerrero.Storage" [@"t - 1"] - 1164.00000000 "MCM" >= 12.96000000 "MCM" )
   THEN
    5.00000000 "cms"
   ELSE
    IF ( $ "Vicente Guerrero.Storage" [@"t - 1"] < 1164.00000000 "MCM" )
    THEN
     0.00000000 "cms"
    ELSE
     "VolumeToFlow"( $ "Vicente Guerrero.Storage" [@"t - 1"] - 1164.00000000 "MCM", @"t" )
    ENDIF
   ENDIF
  ENDIF
 ELSE
  0.00000000 "cms"
 ENDIF;

      $ "Vicente Guerrero.Regulated Spill" [] := IF ( $ "Vicente Guerrero.Storage" [@"t - 1"] > 3910.00000000 "MCM" )
 THEN
  "VolumeToFlow"( $ "Vicente Guerrero.Storage" [@"t - 1"] - 3910.00000000 "MCM", @"t" )
 ELSE
  0.00000000 "cms"
 ENDIF;

    END;

    RULE                 "Rule_Q_Amb_Panuco";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "caudal ambiental.Diversion" [] := $ "Rio Panuco MTY VI.Inflow" [] * 0.10000000;

    END;

    RULE                 "Rule_Panuco";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Rio Panuco MTY VI.Available For Diversion" [] := IF ( $ "Area Metropolitana de Monterrey.Diversion Requested" [] > $ "Area Metropolitana de Monterrey.Diversion" [] )
 THEN
  IF ( $ "Area Metropolitana de Monterrey.Diversion Requested" [] - $ "Area Metropolitana de Monterrey.Diversion" [] < $ "Rio Panuco MTY VI.Diversion Capacity" [] )
  THEN
   $ "Area Metropolitana de Monterrey.Diversion Requested" [] - $ "Area Metropolitana de Monterrey.Diversion" []
  ELSE
   IF ( $ "Rio Panuco MTY VI.Outflow" [] > $ "Rio Panuco MTY VI.Diversion Capacity" [] )
   THEN
    $ "Rio Panuco MTY VI.Diversion Capacity" []
   ELSE
    0.00000000 "cms"
   ENDIF
  ENDIF
 ELSE
  0.00000000 "cms"
 ENDIF;

    END;

    RULE                 "Rule_Libertad";
    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Presa La Libertad.Diversion" [] := IF ( $ "Area Metropolitana de Monterrey.Diversion Requested" [] > $ "Area Metropolitana de Monterrey.Diversion" [] )
 THEN
  IF ( $ "Area Metropolitana de Monterrey.Diversion Requested" [] - $ "Area Metropolitana de Monterrey.Diversion" [] < $ "Presa La Libertad.Diversion Capacity" [] )
  THEN
   $ "Area Metropolitana de Monterrey.Diversion Requested" [] - $ "Area Metropolitana de Monterrey.Diversion" []
  ELSE
   IF ( $ "Presa La Libertad.Conservation Pool Storage" [] > 3.88000000 "MCM" )
   THEN
    $ "Presa La Libertad.Diversion Capacity" []
   ELSE
    0.00000000 "cms"
   ENDIF
