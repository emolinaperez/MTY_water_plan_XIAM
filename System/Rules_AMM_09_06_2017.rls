    DESCRIPTION          "";
    ACTIVE               TRUE;
    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Pozos Area Metropolitana.Pumped Flow" [] := IF ( $ "Pozos Area Metropolitana.Storage" [@"t - 1"] > 1.00000000 "MCM" )
 THEN
  0.87800000 "cms"
 ELSE
  0.87800000 "cms" * 0.50000000
 ENDIF;

      $ "Pozos Area Metropolitana.Pumped Flow" [@"24:00:00 August Max DayOfMonth, Current Year"] := IF ( $ "Pozos Area Metropolitana.Storage" [@"t - 1"] > 1.00000000 "MCM" )
 THEN
  0.87800000 "cms" * 1.50000000
