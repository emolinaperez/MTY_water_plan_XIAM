    RULE_EXEC_CONSTRAINT TRUE;
    BEGIN

      $ "Las Blancas.Release" [] := IF ( $ "Las Blancas.Storage" [@"t - 1"] > 15.52000000 "MCM" )
 THEN
  $ "DataObj1.Caudal Eco Las Blancas" []
